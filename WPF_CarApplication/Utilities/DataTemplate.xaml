<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:WPF_CarApplication.ViewModel"
                    xmlns:view="clr-namespace:WPF_CarApplication.View">

    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <view:Home />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:LoginVM}">
        <view:Login/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:LoggedHomeVM}">
        <view:LoggedHome />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ProductVM}">
        <view:Product/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:TransactionVM}">
        <view:Transaction />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SettingsVM}">
        <view:Setting/>
    </DataTemplate>

</ResourceDictionary>